{
  "name": "wyrihaximus/psr-3-utilities",
  "description": "PSR-3 logger utilities",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com",
      "homepage": "http://wyrihaximus.net/"
    }
  ],
  "require": {
    "php": "^8.4",
    "ext-json": "^8.4",
    "psr/log": "^3"
  },
  "require-dev": {
    "wyrihaximus/makefiles": "^0.7.9",
    "wyrihaximus/test-utilities": "^8.5.0"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\PSR3\\": "src/"
    },
    "files": [
      "src/functions_include.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\PSR3\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "localheinz/composer-normalize": true,
      "ocramius/package-versions": true,
      "phpstan/extension-installer": true,
      "wyrihaximus/makefiles": true,
      "wyrihaximus/test-utilities": true
    },
    "platform": {
      "php": "8.4.13"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "make on-install-or-update || true"
    ],
    "post-update-cmd": [
      "make on-install-or-update || true"
    ]
  }
}
